{
  "rules": {
    ".read": "auth !== null",
    ".write": "auth !== null",
    "serviceAccounts": {
      "$serviceAccountId": {
        ".read": "auth !== null",
        ".write": "auth !== null"
      }
    },
    "usersPublic": {
    },
    "users": {
      "$uid": {
        ".write": "auth !== null && $uid === auth.uid"
      },
    }
  }
}
